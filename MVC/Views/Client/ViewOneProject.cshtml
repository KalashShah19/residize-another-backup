<script>
    $(document).ready(function () {
        const apiUrl = "http://localhost:5293/api/ProjectApi/GetProjectByProjectId?id="@ViewBag.ProjId; // Replace with your actual API URL

        // Fetch data from API
        $.ajax({
            url: apiUrl,
            type: "GET",
            success: function (data) {
                populatePropertyData(data);
                loadAmenities();
               
            },
            error: function (error) {
                
                
                
                console.error("Error fetching property data:", error);
            }
        });

        function loadAmenities(){
           $.ajax({
    url: 'http://localhost:5293/api/ProjectApi/GetProjectAmenitiesByProjectId', // Adjust the controller name
    type: 'GET',
    data: { projectId: @ViewBag.ProjId }, // Sending projectId as query parameter
    success: function (amenities) {
         relatedProperties();
        var iconMap = {
            "Swimming Pool": "fas fa-swimming-pool",
            "Gym": "fas fa-dumbbell",
            "Parking": "fas fa-parking",
            "Child": "fas fa-child",
            "Clubhouse": "fas fa-building",
            "Garden": "fas fa-tree",
            "Security": "fas fa-shield-alt",
            "CCTV": "fas fa-video",
            "Power Backup": "fas fa-battery-full",
            "Lift": "fas fa-sort",
            "Wi-Fi": "fas fa-wifi",
            "Fire Safety": "fas fa-fire-extinguisher",
            "Conference": "fas fa-users",
            "Library": "fas fa-book",
            "Sports Area": "fas fa-futbol",
            "Yoga Room": "fas fa-dove",
            "Kids Zone": "fas fa-child",
            "Jacuzzi": "fas fa-hot-tub",
            "Spa": "fas fa-spa",
            "Theater": "fas fa-film",
            "Tennis Court": "fas fa-table-tennis",
            "Basketball Court": "fas fa-basketball-ball",
            "Temple": "fas fa-place-of-worship"
        };

        // Clear the existing amenities
        $('#amenities-container').empty();
        // Loop through the amenities and create badge elements
        amenities.forEach(function (amenity) {
            console.log(amenity.name);
            var badge = $('<span class="badge"></span>');
            var iconClass = iconMap[amenity.name] || "fas fa-check"; // Use amenity name for the icon
            var icon = $('<i></i>').addClass(iconClass);
            badge.append(icon).append(" " + amenity.name); // Use the name of the amenity
            $('#amenities-container').append(badge);
        });
    },
    error: function (error) {
        console.log('Error fetching amenities: ', error.responseJSON);
    }
});

        }
        function relatedProperties(){
            var dataSource = new kendo.data.DataSource({
        transport: {
            read: {
                url: "http://localhost:5293/api/ProjectApi/GetPropertiesByProjectId?projectId="+@ViewBag.ProjId, // URL endpoint where the data is fetched
                dataType: "json" // Specify that we expect JSON data
            }
        },
        schema: {
        model: {
            fields: {
                propertyId: { type: "number" },
                projectId: { type: "number" },
                propertyName: { type: "string" },
                propertyAmount: { type: "number" },
                propertySize: { type: "number" },
                bedrooms: { type: "number" },
                propertyListingType: { type: "string" },
                propertyPicture: { type: "string" },
                furnished: { type: "string" },
                propertyType: { type: "string" },
                areaType: { type: "string" },
                propertyAge: { type: "number" },
                city: { type: "string" },
                locality: { type: "string" },
                readytoMove: { type: "boolean" },
                address: { type: "string" }
            }
        }
    },
        pageSize: 10, // Optional, if you are paginating
    });
    function bindPropertyCards() {
        var container = $('#property-list');
        container.empty(); // Clear any existing content

        // Get data from DataSource and create the property cards
        dataSource.fetch(function() {
            var data = dataSource.view(); // Get the view (filtered data if any)
            
            // Loop through the data and create property cards dynamically
            data.forEach(function (property) {
    var propertyCard = `
        <div class="card shadow-sm" style="width: 20rem; margin-right: 20px; margin-bottom: 20px; border: none; border-radius: 8px; overflow: hidden;">
            <img class="card-img-top" src="${property.propertyPicture}" alt="${property.propertyName}" style="height: 200px; object-fit: cover;">
            <div class="card-body">
                <h5 class="card-title text-truncate" style="font-weight: bold; font-size: 1.1rem;">${property.propertyName}</h5>
                <p class="text-muted" style="margin: 0; font-size: 0.9rem;">
                    <i class="fas fa-map-marker-alt icon-color" style="color: #007bff;"></i> ${property.locality}, ${property.city}
                </p>
                <hr style="margin: 0.5rem 0;">
                <p style="margin: 0.5rem 0;">
                    <i class="fas fa-tag icon-color" style="color: #28a745;"></i> <b>${property.propertyAmount.toLocaleString()} INR</b>
                </p>
                <p style="margin: 0.5rem 0;">
                    <i class="fas fa-expand icon-color" style="color: #17a2b8;"></i> <b>${property.propertySize} sqft</b>
                </p>
                <p style="margin: 0.5rem 0;">
                    <i class="fas fa-bed icon-color" style="color: #ffc107;"></i> ${property.bedrooms} BHK
                </p>
                <p class="text-muted" style="font-size: 0.9rem;">${property.PropertyAge} years old</p>
                <p class="${property.ReadytoMove ? 'text-success' : 'text-danger'}" style="font-weight: bold;">
                    ${property.ReadytoMove ? 'Ready To Move' : 'Not Ready'}
                </p>
                <div class="d-flex justify-content-between mt-3">
                    <a href="ViewOneProperty?id=${property.propertyId}" class="btn btn-primary btn-sm" style="width: 48%;">View Details</a>
                    <button class="btn btn-secondary btn-sm" style="width: 48%;padding=5px">Request Contact Info</button>
                </div>
            </div>
        </div>
    `;
    container.append(propertyCard); // Append card to the container
});

        });
    }
    bindPropertyCards();
        }

        function populatePropertyData(data) {
            console.log(data);
            // Update carousel
            const carouselItems = $("#carousel-items");
            let carouselContent = "";
            if (data.pictureFiles && data.pictureFiles.length > 0) {
                data.pictureFiles.forEach((picture, index) => {
                    carouselContent += `
                        <div class="carousel-item ${index === 0 ? "active" : ""}">
                            <img src="${picture}" class="d-block w-100" alt="Property Image ${index + 1}">
                        </div>`;
                });
            } else {
                carouselContent = `
                    <div class="carousel-item active">
                        <img src="https://www.shutterstock.com/image-vector/no-photo-blank-image-icon-260nw-1955339317.jpg" class="d-block w-100" alt="Default Property Image">
                    </div>`;
            }
            carouselItems.html(carouselContent);
            $("#photo-count").text(`Photos (0/${data.pictureFiles?.length || 0})`);

            // Update property information
            $("#project-name").text(data.projectName);
            $("#address").text(`${data.address}, ${data.locality}, ${data.city} - ${data.pinCode}`);
            $("#property-age").text(`${data.propertyAge} years`);
            $("#availability-status").text(data.availabilityStatus ? "Ready to move" : "Under construction");
            $("#posted-date").text(new Date(data.postedDate).toLocaleDateString());
            $("#is-verified").text(data.isReraVerified ? "Yes" : "No");

            // Update brochure PDF
            $("#brochure-pdf").attr("data", data.brochureFiles);
            $("#brochure-link").attr("href", data.brochureFiles);

            // Update video
            $("#property-video source").attr("src", data.videoFiles);
            $("#property-video")[0].load();
        }
    });
</script>

