          $("#displayPropAge").text(response.details.propertyAge);
          $("#displayFloor").text(response.property.floor);
          $("#displayAddress").text(response.details.address + ", " + response.details.city);
          $("#displaypropertyAmount").text("‚Çπ" + response.property.propertyAmount);
          $("#displaybathrooms").text(response.property.bathrooms + " Bathrooms, " + response.property.bedrooms + " Badrooms");
          $("#displyPropName").text(response.property.propertyName);
          var postedDate = response.details.postedDate;
          var dateOnly = postedDate.split('T')[0];
          $("#displayDate").text(dateOnly);
          $("#displayLocality").text(response.details.locality);
          $("#displaypropertyListingType").text(response.property.propertyListingType);
          $("#displayareaType").text(response.property.areaType + ":" + response.property.propertySize);
        },
        error: function (xhr, status, error) {
